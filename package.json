{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-gh": "ng build --configuration production && xcopy docs\\browser docs /E /Y && rmdir /s /q docs\\browser && copy docs\\index.html docs\\404.html",
    "deploy-gh": "npm run build-gh && git add . && git commit -m \"Deploy to GitHub Pages\" && git push origin main"
  },
  "private": true,
  "dependencies": {
